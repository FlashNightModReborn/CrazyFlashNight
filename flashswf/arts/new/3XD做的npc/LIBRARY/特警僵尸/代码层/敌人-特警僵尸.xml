			_parent.近战招式 = _parent.横向技能库[random(_parent.横向技能随机数)];
			_parent.状态改变("近战");
		}
		*/
		/*有范围外招式时激活下列内容
		else
		{
			if (_parent.范围外技能随机数 > 0 && random(_parent.攻击欲望 * 15) == 0)
