                <DOMSymbolInstance libraryItemName="判定使用" name="刀口位置3" centerPoint3DX="-21.45" centerPoint3DY="279.65">
                  <matrix>
                    <Matrix a="2.79513549804688" d="5.17520141601563" tx="-35.5" ty="267.15"/>
