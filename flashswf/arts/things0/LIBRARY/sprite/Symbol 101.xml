      <layers>
        <DOMLayer name="Layer 1" color="#E6BA98">
          <frames>
            <DOMFrame index="0" keyMode="9728">
              <elements>
                <DOMSymbolInstance libraryItemName="sprite/Symbol 100" name="战队段位图标" centerPoint3DX="-45.7" centerPoint3DY="22.25">
                  <matrix>
                    <Matrix tx="-68.65" ty="2.35"/>
                  </matrix>
                  <transformationPoint>
                    <Point/>
                  </transformationPoint>
                  <Actionscript>
                    <script><![CDATA[onClipEvent(load){
   function 刷新(战队等级)
   {
      段位等级 = (战队等级 - 1) % 10 + 1;
      段位 = Math.floor((战队等级 - 1) / 10 + 1);
      gotoAndStop(段位);
      if(段位 == 1)
      {
         段位名称 = "粗钢";
      }
      else if(段位 == 2)
      {
         段位名称 = "黄铜";
      }
      else if(段位 == 3)
      {
         段位名称 = "白银";
      }
      else if(段位 == 4)
      {
         段位名称 = "黄金";
      }
      else if(段位 == 5)
      {
         段位名称 = "无畏";
      }
      else if(段位 == 6)
      {
         段位名称 = "传说";
      }
   }
   刷新(Number(_root.我的战队等级));
}
]]></script>
                  </Actionscript>
                </DOMSymbolInstance>
              </elements>
            </DOMFrame>
          </frames>
        </DOMLayer>
