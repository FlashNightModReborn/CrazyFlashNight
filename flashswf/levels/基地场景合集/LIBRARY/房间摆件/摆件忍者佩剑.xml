              <Actionscript>
                <script><![CDATA[function 触发特效()
{
   自机 = _root.获得父节点(this,4);
   耗蓝比例 = 5;
   耗蓝量 = Math.floor(自机.mp满血值 / 100 * 耗蓝比例);
   if(自机.mp >= 耗蓝量)
   {
      自机.mp -= 耗蓝量;
      var _loc3_ = 50;
      var _loc4_ = (Math.random() - 0.5) * 2 * _loc3_;
      var _loc5_ = (Math.random() - 0.5) * 2 * _loc3_;
      var _loc6_ = 自机;
      var _loc7_ = {x:_loc6_._x + _loc4_,y:_loc6_._y + _loc5_};
      声音 = "";
      霰弹值 = 1;
      子弹散射度 = 0;
      发射效果 = "";
      子弹种类 = "苍紫爆炸";
      子弹威力 = 耗蓝量 * 10;
      子弹速度 = 0;
      击中地图效果 = "";
      Z轴攻击范围 = 150;
      击倒率 = 1;
      击中后子弹的效果 = "";
      击退初速度 = 20;
      发射者名 = 自机._name;
      shootX = _loc7_.x;
      Z轴坐标 = shootY = 自机._y;
      _root.子弹区域shoot(声音,霰弹值,子弹散射度,发射效果,子弹种类,子弹威力,子弹速度,Z轴攻击范围,击中地图效果,发射者名,shootX,shootY,Z轴坐标,null,击倒率,击中后子弹的效果,击退初速度);
   }
   else if(自机 == root.gameworld[_root.控制目标])
   {
      root.发布消息("气力不足，难以发挥武器的真正力量……");
   }
}
特效初始cd = 30;
if(!_root.武器特效总计cd)
{
   _root.武器特效总计cd = 30;
}
]]></script>
