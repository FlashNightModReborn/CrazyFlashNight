        <DOMLayer name="Script Layer" color="#96ACB7" current="true" isSelected="true">
          <frames>
            <DOMFrame index="0" keyMode="9728">
              <Actionscript>
                <script><![CDATA[if (!this.击中时触发函数)
{
	this.击中时触发函数 = function()
	{
		//_root.发布消息("触发：" + this.命中对象.man);
		var 暂存敌人 = this.命中对象;
		if (暂存敌人.麻痹值)
		{
			暂存敌人.麻痹值 += 100;
		}
		else
		{
			暂存敌人.麻痹值 = 100;
		}
		if (暂存敌人.麻痹值 >= 100)
		{
			暂存敌人.麻痹值 = 0;
			暂存敌人.状态改变("倒地");
			暂存敌人.麻痹 = true;
			var 麻痹时间 = 8000;
			if (暂存敌人.麻痹抗性)
			{
				麻痹时间 = 麻痹时间 / 暂存敌人.麻痹抗性;
			}
			_root.帧计时器.添加单次任务(function ()
			{
				//_root.发布消息("额外判定：" + 暂存敌人);
				if (暂存敌人.状态 != "倒地")
				{
					暂存敌人.状态改变("倒地");
				}
			},60);
			_root.帧计时器.添加单次任务(function ()
			{
				//_root.发布消息("二次触发：" + 暂存敌人);
				if (暂存敌人.状态 == "倒地" && 暂存敌人.hp > 0)
				{
					暂存敌人.man.stop();
					_root.帧计时器.添加单次任务(function ()
					{
						//_root.发布消息("三次触发：" + 暂存敌人);
						暂存敌人.麻痹 = false;
						if (暂存敌人.状态 == "倒地" || 暂存敌人.hp > 0)
						{
							暂存敌人.man.play();
						}
					},麻痹时间);
				}
			},380);
		}
	};
}
stop();]]></script>
