					_parent.近战招式 = "a2";
					_parent.状态改变("近战");
				}
				else if (_parent._x - _root.gameworld[_parent.攻击目标]._x > -300 and _parent._x - _root.gameworld[_parent.攻击目标]._x < 300)
				{
					_parent.近战招式 = "a0";
					_parent.状态改变("近战");
				}
				else if (random(2) == 0)
				{
					冲刺距离 = _parent._x - _root.gameworld[_parent.攻击目标]._x;
					if (冲刺距离 < 0)
					{
						冲刺距离 = 冲刺距离 * -1;
					}
					冲刺距离 += 50;
					_parent.冲刺轮数总数 = 冲刺距离 / 350;
					_parent.近战招式 = "a1";
					_parent.状态改变("近战");
