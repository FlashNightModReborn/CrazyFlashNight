{
	if (_root.虚拟币 >= amount)
	{
		if (_root.最大宠物格子数 > _root.宠物领养限制)
		{
			_root.虚拟币 -= amount;
			_root.最上层发布文字提示("支付成功！");
			text = "购买成功！";
			_root.播放音效("收银机.mp3");
			_root.开宠物格子();
			_root.获取虚拟币值();
			_parent.排列宠物图标();
		}
		else
		{
			_root.最上层发布文字提示(_root.获得翻译("超出数量限制"));
		}
	}
	else
	{
		_root.最上层发布文字提示(_root.获得翻译("K点不足！"));
	}
}
function 虚拟币支付加体力(amount, 商品编号, 格子位置号)
{
	if (_root.金钱 >= amount)
	{
		_root.金钱 -= amount;
		_root.最上层发布文字提示(_root.获得翻译("支付成功！"));
		text = _root.获得翻译("购买成功！");
		_root.播放音效("收银机.mp3");
		_root.宠物信息[宠物信息数组号][2] = 200;
		_root.获取虚拟币值();
		_parent.排列宠物图标();
		gotoAndStop("成功");
	}
	else
	{
		_root.最上层发布文字提示(_root.获得翻译("金钱不足！"));
		text = _root.获得翻译("金钱不足！");
	}
}
function 宠物升级(该宠物信息, 格子位置号, 强化石数量)
{
	if (_root.singleSubmit("战宠灵石", 强化石数量))
	{
		var 新等级 = _root.宠物信息[格子位置号][1] + 1;
		_root.宠物信息[格子位置号][1] = 新等级;
		//_root.最上层发布文字提示(_root.获得翻译("强化成功！"));
		//计算经验
		var 宠物数据 = _root.宠物库[_root.宠物信息[格子位置号][0]];
		var 宠物兵种 = 宠物数据.Identifier;
		_root.宠物信息[格子位置号][5].宠物升级所需经验 = _root.战宠UI函数.计算战宠升级所需经验(宠物兵种,新等级);
		// 刷新在场宠物
		for(i=0; i<_root.宠物mc库.length; i++){
			if(_root.宠物mc库[i].宠物属性.宠物信息数组号 == 宠物信息数组号){
				_root.宠物升级加载(i);
				break;
			}
		}
		/*
		var 敌人属性 = _root.敌人属性表[宠物兵种];
		if(敌人属性.最小经验值){
			_root.宠物信息[格子位置号][5].宠物升级所需经验 = (敌人属性.最小经验值 + ((敌人属性.最大经验值 - 敌人属性.最小经验值) / (_root.最大等级 - 1)) * 新等级) * 新等级;
			_root.发布消息(_root.宠物信息[格子位置号][5].宠物升级所需经验 + "/" + 敌人属性.最小经验值 + "/" + 敌人属性.等级);
		}*/
		_root.最上层发布文字提示("战宠升级！战宠灵石 -" + 强化石数量);
		_parent.排列宠物图标();
	}
	else
	{
		_root.最上层发布文字提示(_root.获得翻译("战宠灵石不足，无法强化！"));
	}
}
function 删除宠物(该宠物信息, 格子位置号)
{
	_root.最上层发布文字提示(_root.获得翻译("删除成功！"));
	_root.删除场景宠物();
	计算返还战宠灵石 = 0;
	if (_root.宠物信息[宠物信息数组号][5].宠物升级所需经验)
	{
		计算返还战宠灵石 = Math.floor(Math.sqrt(_root.宠物信息[宠物信息数组号][1]) * 0.8 * _root.宠物信息[宠物信息数组号][5].宠物升级所需经验 / 10000);
	}
	if (计算返还战宠灵石)
	{
		if (_root.singleAcquire("战宠灵石",计算返还战宠灵石))
		{
			_root.发布消息("战宠已解散，返还战宠灵石" + 计算返还战宠灵石 + "个");
		}
		else
		{
			_root.商城已购买物品.push(['S20130922000001', '战宠灵石', '常用材料', 60, 计算返还战宠灵石]);
			_root.发布消息("战宠已解散，返还战宠灵石" + 计算返还战宠灵石 + "个。背包已满，已存放于商城");
		}
	}
	_root.宠物信息[宠物信息数组号] = [];
	_parent.排列宠物图标();
}
