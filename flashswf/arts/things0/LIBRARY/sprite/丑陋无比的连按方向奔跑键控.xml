                  <Actionscript>
                    <script><![CDATA[// 鼠标按下事件：更新 aa 属性，加上 "m"
onClipEvent(mouseDown) {
    _parent.aa = _parent.aa.substr(1, 9) + "m";
}

// 每帧事件，根据 aa 中的指令执行对应动作
onClipEvent(enterFrame) {
    // 从 aa 中提取指令，注意部分指令为单字符（如 f），统一转换为两字符的情况
    var cmd:String = _parent.aa.substr(8, 2);
    if (cmd.charAt(0) == "f") {
        cmd = "f";
    }
    
    // 构建动作映射表，表中只保存参数，不嵌入大量函数
    var actionMap:Object = {
        // 跑动相关：只有在没有射击中的情况下才执行
        dd: { type: "run", zhaoshu: "右跑" },
        aa: { type: "run", zhaoshu: "左跑" },
        
        // 技能相关
        f:  { type: "skill", zhaoshu: "技能闪避", skillId: -1 },
        zw: { type: "skill", zhaoshu: "技能w", skillId: 0 },
        zs: { type: "skill", zhaoshu: "技能s", skillId: 1 },
        
        // 方向依赖的技能
        za: { 
            type: "skill", 
            // 如果方向为“右”，则表示后技能；如果方向为“左”，则表示前技能
            zhaoshu: { 右: "技能后", 左: "技能前" },
            skillId: { 右: 2, 左: 3 }
        },
        zd: { 
            type: "skill", 
            // 如果方向为“右”，则表示前技能；如果方向为“左”，则表示后技能
            zhaoshu: { 右: "技能前", 左: "技能后" },
            skillId: { 右: 3, 左: 2 }
        },
        
        // 取消技能，不需要调用发动技能
        zz: { type: "none", zhaoshu: "取消技能" },
        
        // 其它技能
        zc: { type: "skill", zhaoshu: "技能c", skillId: 4 },
        zv: { type: "skill", zhaoshu: "技能v", skillId: 5 }
    };
    
    // 如果映射表中存在该命令，则根据类型执行公共逻辑
    if (actionMap[cmd] != undefined) {
        var actionObj:Object = actionMap[cmd];
        // 如果需要判断角色朝向，则取当前方向
        var direction:String = _parent._parent.方向;
        
        // 跑动命令，需先确认当前没有射击中
        if (actionObj.type == "run") {
            if (!_parent._parent.射击中) {
                _parent.zhaoshu = actionObj.zhaoshu;
                _parent._parent.状态改变(_parent._parent.攻击模式 + "跑");
            }
        }
        // 技能命令
        else if (actionObj.type == "skill") {
            // 如果技能参数依赖于方向，则分别处理
            if (typeof(actionObj.zhaoshu) == "object") {
                _parent.zhaoshu = actionObj.zhaoshu[direction];
                _root.发动技能(_parent._parent.操控编号, actionObj.skillId[direction]);
            } else {
                _parent.zhaoshu = actionObj.zhaoshu;
                _root.发动技能(_parent._parent.操控编号, actionObj.skillId);
            }
        }
        // 对于不需要调用额外函数的命令，例如取消技能
        else if (actionObj.type == "none") {
            _parent.zhaoshu = actionObj.zhaoshu;
        }
        
        // 公共操作：执行完命令后，更新 aa 属性
        _parent.aa = _parent.aa.substr(1, 9) + "-";
    }
}
]]></script>
