                  <Actionscript>
                    <script><![CDATA[onClipEvent(enterFrame){
	if(!_parent.发射者 || _parent.发射者.hp <=0){
		_parent.gotoAndStop("结束");
	}else{
		//_parent.发射者.自瞄中 = true;
	}
	强制切换 = false;
	if(_parent.发射者._name == _root.控制目标){
		if(Key.isDown(_root.互动键)){
			已按下切换键 = true;
		}else if(已按下切换键){
			已按下切换键 = false;
			强制切换 = true;
		}
	}
	if(_parent.追踪目标 && _parent.追踪目标.hp>0 && !强制切换){
   		_parent._y = _parent.追踪目标._y;
  		_parent._x = _parent.追踪目标._x;
		_parent.发射者.攻击目标 = _parent.追踪目标._name;
	}else if(_parent.索敌目标列表.length > 0){
		if(_parent.敌人顺序 >= _parent.索敌目标列表.length -1){
			_parent.敌人顺序 = -1;
		}
		for(var i = _parent.敌人顺序 + 1; i < _parent.索敌目标列表.length; i++){
			if(_parent.索敌目标列表[i].敌人.hp > 0){
				_parent.追踪目标 = _parent.索敌目标列表[i].敌人;
				_parent.发射者.攻击目标 = _parent.追踪目标._name;
				_parent.敌人顺序 = i;
				break;
			}
		}
		if(!_parent.追踪目标 || _parent.追踪目标.hp<=0){
			_parent.是否需要刷新 = true;
		}
	}else{
		_parent.是否需要刷新 = true;
	}
}
onClipEvent(unload){
	//_parent.发射者.自瞄中 = false;
}
]]></script>
