					_parent.状态改变("近战");
				}
			}
		}
		*/
		/*有身后招式时激活下列内容
		else if (距离X < 130 && 距离Z <= _parent.y轴攻击范围)
		{
			if (_parent.身后技能随机数 > 0 && random(_parent.攻击欲望) == 0)
