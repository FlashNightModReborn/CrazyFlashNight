		{
			play();
			if (random(_parent.攻击欲望) == 0)
			{
				/*有残血技能时激活下方内容，注意调整触发的距离X的范围
				if(_parent.hp <  _parent.hp满血值*0.3 && 距离X >= 0 && 距离X < 500  && _parent.残血技能随机数 > 0)
