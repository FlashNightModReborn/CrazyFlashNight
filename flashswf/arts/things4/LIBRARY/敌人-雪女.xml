//尝试重构魔女的击退函数：始终有较小的摩擦力，并会稍微斜向位移
被击移动 = function (移动方向, 速度, 摩擦力)
{
	if (免疫击退)
	{
		return;
	}
	移动钝感硬直(_root.钝感硬直时间);
	减速度 = 摩擦力;
	if (垂直被击移动方向 != "上" and 垂直被击移动方向 != "下")
	{
		垂直被击移动方向 = random(2) == 0 ? "上" : "下";
	}
	if (减速度 > 0.5)
	{
		减速度 = 0.5;
	}
	speed = 速度;
	this.onEnterFrame = function()
	{
		if (硬直中 == false)
		{
			speed -= 减速度;
			this.移动(移动方向,speed);
			this.移动(垂直被击移动方向, 0.2 * speed);
			if (speed <= 0)
			{
				delete this.onEnterFrame;
				delete 垂直被击移动方向;
			}
		}
	};
};

初始化敌人模板 = _root.初始化敌人模板;
初始化敌人模板();
