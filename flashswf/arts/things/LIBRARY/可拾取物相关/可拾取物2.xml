                <script><![CDATA[mytext = displayName + " X" + 数量;
if (物品名 !== "金钱" && 物品名 !== "K点")
{
	var type = itemData.type;
	if (type === "武器" || type === "防具")
	{
		if (数量 > 1)
		{
			mytext = displayName + " +" + 数量;
		}
		else
		{
			mytext = displayName;
		}
	}
}

this.物品图标.游戏世界 = _root.gameworld;
this.物品图标.attachMovie("图标-" + this.itemData.icon,"图标",0);
if (this.物品图标.图标._x != undefined)
{
	this.物品图标.基本款._visible = false;
	this.物品图标.图标.gotoAndStop(2);
}
else
{
	this.物品图标.基本款._visible = true;
}

this.xmov = !isNaN(this.xmov) ? this.xmov : random(11) - 5;
//this.旋转速度 = 90 - random(180); //未使用
this.起始Y = this._y;
this.垂直速度 = !isNaN(this.垂直速度) ? this.垂直速度 : -(random(5)) - 10;



Z轴坐标 = this._y;

if (this.在飞)
{
	gotoAndStop("飞出");
	this.onEnterFrame = function()
	{

		if (this._y - 起始Y > 10)
		{
			if (Math.floor(垂直速度) > 1)
			{
				垂直速度 = (-垂直速度) / 2;
			}
			else
			{
				delete this.onEnterFrame;
				this.gotoAndStop("静止");
			}
		}
		var temp_x = this._x + xmov;
		var _loc4_ = {x:temp_x, y:起始Y};
		游戏世界.localToGlobal(_loc4_);
		var xx = _loc4_.x;
		var yy = _loc4_.y;
		if (xmov > 0 && temp_x < _root.Xmax && !游戏世界.地图.hitTest(xx, yy, true))
		{
			this._x = temp_x;
		}
		else if (xmov < 0 && temp_x > _root.Xmin && !游戏世界.地图.hitTest(xx, yy, true))
		{
			this._x = temp_x;
		}
		else
		{
			xmov = -Math.floor(xmov * 0.7);//撞墙弹回
		}
		if (_root.重力加速度 > 3)
		{
			this.垂直速度 += 3;
		}
		else
		{
			this.垂直速度 += _root.重力加速度;
		}
		this._y += 垂直速度;
	};
}
else
{
	gotoAndStop("静止");
}]]></script>
