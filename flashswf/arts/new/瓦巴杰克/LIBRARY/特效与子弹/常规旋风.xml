                <script><![CDATA[this._rotation = 0;

function 移动敌人(预计移动距离X, 预计移动距离Y)
{
	var 游戏世界 = _root.gameworld;
	var 发射对象 = 游戏世界[this.发射者名];
	var 遍历敌人表 = _root.帧计时器.获取敌人缓存(发射对象, 5);
	for (i = 0; i < 遍历敌人表.length; ++i)
	{
		var 移动目标 = 遍历敌人表[i];
		var 距离X = this._x - 移动目标._x;
		var 距离Y = this._y - 移动目标._y;
		var 方向X = "右";
		var 方向Y = "下";
		if (距离X < 0)
		{
			方向X = "左";
			距离X = -距离X;
		}
		if (距离Y < 0)
		{
			方向Y = "上";
			距离Y = -距离Y;
		}
		if (移动目标.hp > 0 && 距离X < 500 && 距离Y < 200)
		{
			var 移动距离X = 距离X / 3;
			var 移动距离Y = 距离Y / 3;

			if (移动距离X < 预计移动距离X)
			{
				移动距离X = 预计移动距离X;
			}
			if (移动距离X > 距离X)
			{
				移动距离X = 距离X;
			}
			if (移动距离Y < 预计移动距离Y)
			{
				移动距离Y = 预计移动距离Y;
			}
			if (移动距离Y > 距离Y)
			{
				移动距离Y = 距离Y;
			}
			移动目标.强制移动(方向X,移动距离X);
			移动目标.强制移动(方向Y,移动距离Y);
		}
	}
}]]></script>
