				{
					_parent.近战招式 = _parent.超远程技能库[random(_parent.超远程技能随机数)];
					_parent.状态改变("近战");
				}
			}

		}
		/*有高空招式时激活下方内容
		else if (敌方高度 > 80 and _parent.高空技能随机数 > 0 and 距离Z <= _parent.y轴攻击范围 * 5)
		{

			if (random(_parent.攻击欲望) == 0)
			{
