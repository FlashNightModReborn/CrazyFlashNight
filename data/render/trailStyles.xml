        <lineWidth>3</lineWidth>        <!-- 略微粗一些，突出轮廓感 -->
        <fillOpacity>80</fillOpacity>        <!-- 让能量感更明显 -->
        <lineOpacity>70</lineOpacity>        <!-- 发光外轮廓稍弱，避免抢镜 -->
