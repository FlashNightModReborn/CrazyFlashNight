              <Actionscript>
                <script><![CDATA[this._rotation = 0;

function 获取移动对象()
{
	var 游戏世界 = _root.gameworld;
	var 发射对象 = 游戏世界[this.发射者名];
	this.遍历敌人表 = _root.帧计时器.获取敌人缓存(发射对象, 5);
	this.目标移动距离X = [];
	this.目标移动距离Y = [];
	for (i = 0; i < 遍历敌人表.length; ++i)
	{
		var 移动目标 = 遍历敌人表[i];
		if (移动目标.hp > 0)
		{
			//_root.gameworld[each].状态改变("击倒");
			var 当前目标移动距离X = (this._x - 移动目标._x) / 5;
			var 当前目标移动距离Y = (this._y - 移动目标._y) / 5;


			if (当前目标移动距离X < 0)
			{
				当前目标移动距离X = -当前目标移动距离X;
			}
			if (当前目标移动距离X < 150)
			{
				当前目标移动距离X = 150;
			}
			if (当前目标移动距离Y < 0)
			{
				当前目标移动距离Y = -当前目标移动距离Y;
			}
			if (当前目标移动距离Y < 100)
			{
				当前目标移动距离Y = 100;
			}
			目标移动距离X.push(当前目标移动距离X);
			目标移动距离Y.push(当前目标移动距离Y);
		}
	}
}


function 移动敌人()
{
	for (i = 0; i < 遍历敌人表.length; ++i)
	{
		var 移动目标 = 遍历敌人表[i];
		if (移动目标.hp <= 0)
		{
			continue;
		}
		var 移动距离X = 目标移动距离X[i];
		var 移动距离Y = 目标移动距离Y[i];
		var 距离X = this._x - 移动目标._x;
		var 距离Y = this._y - 移动目标._y;
		var 方向X = "右";
		var 方向Y = "下";

		if (距离X < 0)
		{
			方向X = "左";
			距离X = -距离X;
		}
		if (移动距离X > 距离X)
		{
			移动距离X = 距离X;
		}
		if (距离Y < 0)
		{
			方向Y = "上";
			距离Y = -距离Y;
		}
		if (移动距离Y > 距离Y)
		{
			移动距离Y = 距离Y;
		}
		移动目标.强制移动(方向X,移动距离X);
		移动目标.强制移动(方向Y,移动距离Y);
		移动目标.状态改变("击倒");
	}
}

获取移动对象();

var 段数计数 = 1;
if (!子弹威力)
{
	总段数 = 3;
}
else
{
	总段数 = 2.5 + 子弹威力 / 200;
}


this.onUnload = function()
{
	if (_root.重力加速度基准)
	{
		_root.重力加速度 = _root.重力加速度基准;
	}
	else
	{
		_root.重力加速度 = 0.8;
	}//_root.发布消息("反重力效果结束");
};]]></script>
