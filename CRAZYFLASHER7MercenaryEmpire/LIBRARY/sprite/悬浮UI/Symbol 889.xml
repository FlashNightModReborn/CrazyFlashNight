		是否刷新 = true;
	}
	else if (_root.佣兵是否出战信息[_parent.同伴数据数组号] == 1)
	{
		显示文字 = _root.获得翻译("出战");
		_parent.状态 = "<FONT COLOR=\'#66FF00\'>" + _root.获得翻译("出战中") + "</FONT>";
		_root.佣兵是否出战信息[_parent.同伴数据数组号] = 0;
		是否刷新 = true;
	}
	else if (_root.佣兵是否出战信息[_parent.同伴数据数组号] == -1)
	{
		if (_root.singleSubmit("复活币", 1))
		{
			显示文字 = _root.获得翻译("出战");
			_parent.状态 = "<FONT COLOR=\'#66FF00\'>" + _root.获得翻译("出战中") + "</FONT>";
			_root.佣兵是否出战信息[_parent.同伴数据数组号] = 1;
			是否刷新 = true;
		}
		else
		{
			_root.发布消息(_root.获得翻译("复活失败，请购买复活币！"));
		}
	}
	if (是否刷新 == true)
	{
		_root.删除场景上佣兵();
		_root.加载佣兵(_root.gameworld[_root.控制目标]._x,_root.gameworld[_root.控制目标]._y);
		_parent._parent.排列佣兵图标();
	}
}
onClipEvent (load) {
	if (_root.佣兵是否出战信息[_parent.同伴数据数组号] == 0)
