              <Actionscript>
                <script><![CDATA[this._rotation = 0;
目标数组 = [];
for (var each in _root.gameworld)
{
	var 当前目标 = _root.gameworld[each];
	if (当前目标.是否为敌人 == !_parent._parent.是否为敌人 and 当前目标.hp > 0)
	{
		目标数组.push(each);
		当前目标.stop();
		当前目标.被时停 = true;
		/*
		 当前目标.左行=0;
		 当前目标.右行=0;
		 当前目标.上行=0;
		 当前目标.下行=0;
		 */
		if (当前目标.行走X速度 != 0)
		{
			当前目标.temp_行走X速度 = 当前目标.行走X速度;
			当前目标.行走X速度 = 0;
			当前目标.temp_行走Y速度 = 当前目标.行走Y速度;
			当前目标.行走Y速度 = 0;
			当前目标.temp_跑X速度 = 当前目标.跑X速度;
			当前目标.跑X速度 = 0;
			当前目标.temp_跑Y速度 = 当前目标.跑Y速度;
			当前目标.跑Y速度 = 0;
		}

		for (var eachObj in 当前目标)
		{
			当前目标[eachObj].stop();
		}
	}
}
var 秒数计数 = 0;
if (!子弹威力)
{
	总秒数 = 2;
}
else
{
	总秒数 = 1 + 子弹威力 * 0.8;
}


this.onUnload = function()
{
	for (i = 0; i < 目标数组.length; i++)
	{
		each = 目标数组[i];
		var 当前目标 = _root.gameworld[each];
		当前目标.被时停 = false;
		if (当前目标.hp > 0)
		{

			if (当前目标.行走X速度 == 0 && 当前目标.temp_行走X速度 > 0)
			{
				当前目标.行走X速度 = 当前目标.temp_行走X速度;
				当前目标.行走Y速度 = 当前目标.temp_行走Y速度;
				当前目标.跑X速度 = 当前目标.temp_跑X速度;
				当前目标.跑Y速度 = 当前目标.temp_跑Y速度;
			}

			for (var eachObj in 当前目标)
			{
				当前目标[eachObj].play();
			}
		}
	}
	_root.发布消息("时停结束");
};]]></script>
              </Actionscript>
