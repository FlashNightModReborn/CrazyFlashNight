onClipEvent(enterFrame){
   // 不稳定的旋转加速
   this.angle += 2;
   var spinSpeed = 25 + Math.sin(this.angle * 0.1) * 20;
   this._rotation += spinSpeed;

   // 3D倾斜摆动（模拟失衡）
   this.tiltAngle += 6;
   var tilt = Math.sin(this.tiltAngle * Math.PI / 180) * 40;

   // X轴和Y轴同时变化模拟3D翻转（基于初始缩放）
   this._xscale = this.baseXScale * (1 + tilt * 0.005);
   this._yscale = this.baseYScale * (1 - tilt * 0.008);

   // 轻微位置抖动（过载不稳定）
   this._x += Math.random() * 1.5 - 0.75;
   this._y += Math.random() * 1.5 - 0.75;
}]]></script>
