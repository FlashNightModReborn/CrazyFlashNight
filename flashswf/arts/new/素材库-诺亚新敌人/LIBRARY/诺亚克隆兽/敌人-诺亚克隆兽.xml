				if (距离X < 200 && 距离Z <= _parent.y轴攻击范围)
				{
					_parent.高空招式 = _parent.高空技能库[random(_parent.高空技能随机数)];
					if (敌方高度 > 200 and _parent.等级 >= 50 and _parent.高空招式 == "中空连招")
					{
						_parent.高空招式 = "高空连招";
					}
					_parent.状态改变("空手跳");
				}
			}
		}
		*/
